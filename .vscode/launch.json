{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Excel (Debug)",
            "type": "clr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "C:\\Program Files\\Microsoft Office\\root\\Office16\\EXCEL.EXE",
            "args": ["/x", "${workspaceFolder}\\bin\\Debug\\net9.0-windows\\NumDesToolsFun-AddIn64.xll"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "externalTerminal",
            "env": {
                "EXCELDNA_DEBUG": "1"
            }
        },
        {
            "name": "Attach to Excel",
            "type": "clr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}